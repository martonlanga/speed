{
  "name": "speed",
  "version": "1.0",
  "description": "speed baaby",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "128": "images/speed.png"
  },
  "commands": {
    "increment-speed": {
      "suggested_key": {
        "default": "Alt+L",
        "mac": "Alt+L"
      },
      "description": "Increment first video on page if found by 0.25"
    },
    "decrement-speed": {
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Alt+J"
      },
      "description": "Decrement first video on page if found by 0.25"
    },
    "pause-play": {
      "suggested_key": {
        "default": "Alt+K",
        "mac": "Alt+K"
      },
      "description": "Pause or Play first video on page if found"
    }
  },
  "manifest_version": 2
}
